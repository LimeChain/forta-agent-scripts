{
  "name": "idlefi-best-yield-token-mint-or-borrow-with-flashloan",
  "version": "0.0.1",
  "description": "Detects if there is a mint or redeem of idleToken in the same transaction as a flashloan",
  "chainIds": [ 1 ],
  "repository": "https://github.com/LimeChain/forta-agent-scripts",
  "scripts": {
    "start": "npm run start:dev",
    "start:dev": "nodemon --watch src --watch forta.config.json -e js,json --exec \"forta-agent run\"",
    "start:prod": "forta-agent run --prod",
    "tx": "forta-agent run --tx",
    "block": "forta-agent run --block",
    "range": "forta-agent run --range",
    "file": "forta-agent run --file",
    "publish": "forta-agent publish",
    "push": "forta-agent push",
    "disable": "forta-agent disable",
    "enable": "forta-agent enable",
    "keyfile": "forta-agent keyfile",
    "test": "jest"
  },
  "dependencies": {
    "ethers-multicall": "^0.2.2",
    "forta-agent": "^0.0.41",
    "forta-flashloan-detector": "^1.0.3"
  },
  "devDependencies": {
    "jest": "^27.0.6",
    "nodemon": "^2.0.8"
  }
}
